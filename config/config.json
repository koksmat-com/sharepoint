{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "nexi-button-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/nexiButton/NexiButtonWebPart.js",
          "manifest": "./src/webparts/nexiButton/NexiButtonWebPart.manifest.json"
        }
      ]
    },
    "nexi-tabs-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/nexiTabs/NexiTabsWebPart.js",
          "manifest": "./src/webparts/nexiTabs/NexiTabsWebPart.manifest.json"
        }
      ]
    },
    "nexi-top-nav-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/nexiTopNav/NexiTopNavApplicationCustomizer.js",
          "manifest": "./src/extensions/nexiTopNav/NexiTopNavApplicationCustomizer.manifest.json"
        }
      ]
    },
    "nexi-sticky-table-of-contents-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/nexiStickyTableOfContents/NexiStickyTableOfContentsWebPart.js",
          "manifest": "./src/webparts/nexiStickyTableOfContents/NexiStickyTableOfContentsWebPart.manifest.json"
        }
      ]
    },
    "nexi-navigation-setup-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/nexiNavigationSetup/NexiNavigationSetupWebPart.js",
          "manifest": "./src/webparts/nexiNavigationSetup/NexiNavigationSetupWebPart.manifest.json"
        }
      ]
    },
    "links-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/links/LinksWebPart.js",
          "manifest": "./src/webparts/links/LinksWebPart.manifest.json"
        }
      ]
    },
    "news-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/news/NewsWebPart.js",
          "manifest": "./src/webparts/news/NewsWebPart.manifest.json"
        }
      ]
    },
    "rolluppage-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/rolluppage/RolluppageWebPart.js",
          "manifest": "./src/webparts/rolluppage/RolluppageWebPart.manifest.json"
        }
      ]
    },
    "matrix-view-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/matrixView/MatrixViewWebPart.js",
          "manifest": "./src/webparts/matrixView/MatrixViewWebPart.manifest.json"
        }
      ]
    },
    "shared-page-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sharedPage/SharedPageWebPart.js",
          "manifest": "./src/webparts/sharedPage/SharedPageWebPart.manifest.json"
        }
      ]
    },
    "magic-button-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/magicButton/MagicButtonWebPart.js",
          "manifest": "./src/webparts/magicButton/MagicButtonWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "NexiButtonWebPartStrings": "lib/webparts/nexiButton/loc/{locale}.js",
    "NexiTabsWebPartStrings": "lib/webparts/nexiTabs/loc/{locale}.js",
    "NexiTopNavApplicationCustomizerStrings": "lib/extensions/nexiTopNav/loc/{locale}.js",
    "NexiStickyTableOfContentsWebPartStrings": "lib/webparts/nexiStickyTableOfContents/loc/{locale}.js",
    "NexiNavigationSetupWebPartStrings": "lib/webparts/nexiNavigationSetup/loc/{locale}.js",
    "LinksWebPartStrings": "lib/webparts/links/loc/{locale}.js",
    "NewsWebPartStrings": "lib/webparts/news/loc/{locale}.js",
    "RolluppageWebPartStrings": "lib/webparts/rolluppage/loc/{locale}.js",
    "MatrixViewWebPartStrings": "lib/webparts/matrixView/loc/{locale}.js",
    "SharedPageWebPartStrings": "lib/webparts/sharedPage/loc/{locale}.js",
    "MagicButtonWebPartStrings": "lib/webparts/magicButton/loc/{locale}.js"
  }
}
